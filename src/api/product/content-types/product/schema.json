{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "price": {
      "type": "decimal"
    },
    "isAvailable": {
      "type": "string"
    },
    "isAvailableNumber": {
      "type": "integer"
    },
    "SKU": {
      "type": "string"
    },
    "brand": {
      "type": "string"
    },
    "colors": {
      "type": "enumeration",
      "enum": [
        "wood",
        "dark-wood",
        "white",
        "black",
        "red",
        "blue",
        "green",
        "yellow"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "img2": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isNew": {
      "type": "boolean",
      "default": false
    },
    "type": {
      "type": "string"
    },
    "isSale": {
      "type": "boolean",
      "default": true
    },
    "oldPrice": {
      "type": "decimal"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "sub_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sub-category.sub-category"
    }
  }
}
